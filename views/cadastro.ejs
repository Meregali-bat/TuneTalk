<div id="bodyLogin">
    <div id="bodyInputs">
        
            <form action="/cadastrar" method="post" id="container__central__form" enctype="multipart/form-data">
                <label for="container__central__form__imagem" id="container__central__form__label">
                    <i id="icone" class="bi bi-person-fill"></i>
                    <span class="picture__img"></span>
                </label>
                <input type="file" name="imagem" id="container__central__form__imagem">
                <div id="container__central__form__nomeSobrenome">
                    <input type="text" name="nome" id="container__central__form__nome" placeholder="Nome" required>
                </div>
        
        <input type="text" name="email" id="container__central__form__email" placeholder="E-mail" required>
        <input type="password" id="container__central__form__senha" placeholder="Senha">
        <small id="passwordError" style="color: red;"></small>
        <input type="password" id="container__central__form__confirmarSenha" placeholder="Confirmar Senha">
        <input type="hidden" name="fotoPerfilUrl" id="fotoPerfilUrl">
        <input type="hidden" name="publicId" id="publicId">
        <button type="submit" id="container__central__form__button">Criar conta</button>
        </form>
        <p class="novoUsuario">já tem uma conta? <a class="novoUsuario" href="/login">Faça login aqui</a></p>
    </div>
</div>
